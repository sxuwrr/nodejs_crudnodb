<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>

<body>
    <h1>User List</h1>
    <a href="/users/add">Add User</a>
    <ul>
        <% users.forEach(user=> { %>
            <li>
                <%= user.name %> (<%= user.email %>)
                        <a href="/users/edit/<%= user.id %>">Edit</a>
                        <a href="/users/delete/<%= user.id %>">Delete</a>
            </li>
            <% }) %>
    </ul>
</body>

</html>